  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:e2e": "jest tests/e2e",
    "ci": "pnpm lint:check && pnpm format:check && pnpm type-check && pnpm build && pnpm test",
    "docker:build": "docker build -t url-shortener .",
    "docker:run": "docker run -p 3000:3000 -p 3001:3001 url-shortener",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "packageManager": "pnpm@10.9.0",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
